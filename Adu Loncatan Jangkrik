#include <stdio.h>

int main(){
    int a,b,c,d;
    printf("Masukkan angka = ");
    scanf("%d %d %d %d",&a,&b,&c,&d);
    
    if( a<0 || a>c || c>1000 ){
    	printf("Error");
    	return 0;
	}
    else if( b<1 || b>1000 ){
    	printf("\nError");
    	return 0;
	}
    else if( d<1 || d>1000 ){
   		printf("\nError");
   		return 0;
    }
     while(a!=c){
    	a=a+b;
    	c=c+d;
    
    if(a==c){
    printf("YES");
        break;
    }
    else if(a>1000000){
    printf("NO");
    break;}
    }
    
    
    return 0;
}
